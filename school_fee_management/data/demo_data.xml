        <!-- Demo Parent Users -->
        <record id="demo_parent_john" model="res.partner">
            <field name="name">John Smith</field>
            <field name="email">john.smith@example.com</field>
            <field name="phone">+1-555-0101</field>
            <field name="street">123 Maple Street</field>
            <field name="city">Springfield</field>
            <field name="zip">12345</field>
            <field name="country_id" ref="base.us"/>
        </record>

        <record id="demo_parent_mary" model="res.partner">
